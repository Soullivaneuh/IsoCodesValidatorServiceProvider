{
    "name": "sllh/iso-codes-validator-service-provider",
    "type": "library",
    "description": "Silex service provider for IsoCodesValidator",
    "keywords": ["Silex", "ISO codes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan Senechal",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "^1.1.1",
        "symfony/translation": "~2.3",
        "sllh/iso-codes-validator": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "symfony/phpunit-bridge": "~2.7",
        "satooshi/php-coveralls": "~0.6",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "conflict": {
        "symfony/config": "<=2.0.4"
    },
    "autoload": {
        "psr-4": { "SLLH\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
